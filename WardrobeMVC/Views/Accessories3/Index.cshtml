@model IEnumerable<WardrobeMVC.Models.Accessories3>

@{
    ViewBag.Title = "Index";
}

<h2 style="font-family: 'Coda Caption', sans-serif;">More Accessories</h2>

<p style="font-family: 'Saira Condensed', sans-serif;font-size:20px;">
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Accessory3Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Accessory3Photo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Accessory3Types.Accessory3TypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Accessory3Colors.Accessory3ColorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Accessory3Occassions.Accessory3OccassionName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Accessory3Seasons.Accessory3SeasonsName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Accessory3Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Accessory3Photo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Accessory3Types.Accessory3TypeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Accessory3Colors.Accessory3ColorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Accessory3Occassions.Accessory3OccassionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Accessory3Seasons.Accessory3SeasonsName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Accessory3ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Accessory3ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Accessory3ID })
        </td>
    </tr>
}

</table>
